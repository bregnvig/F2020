  "scripts": {
    "ng": "nx",
    "nx": "nx",
    "start": "ng serve ui --liveReload false",
    "build": "nx build",
    "build-with-stats": "nx build --stats-json",
    "test": "ng test",
    "lint": "nx workspace-lint && ng lint",
    "kill-ports": "npx kill-port --port 9229,9099,5001,8080,9000,5000,8085,9199,9299,4000,4400,4500",
    "deploy:all": "npm run deploy:hosting:production && npm run deploy:hosting:develop && npm run deploy:functions:production && npm run deploy:functions:develop",
    "deploy:hosting:all": "npm run deploy:hosting:production && npm run deploy:hosting:develop",
    "deploy:hosting:develop": "firebase use default && nx build ui --configuration test && firebase deploy --only hosting",
    "deploy:hosting:production": "firebase use production && nx build ui --prod && firebase deploy --only hosting",
    "deploy:functions:all": "npm run deploy:functions:production && npm run deploy:functions:develop",
    "deploy:functions:develop": "firebase use default && nx build cloud && firebase deploy --only functions",
    "deploy:functions:production": "firebase use production && nx build cloud && firebase deploy --only functions",
    "deploy:rules:develop": "firebase use default && firebase deploy --only firestore:rules",
    "deploy:rules:production": "firebase use production && firebase deploy --only firestore:rules",
    "e2e": "ng e2e",
    "analyze:ui": "webpack-bundle-analyzer dist/apps/ui/stats.json",
    "affected:apps": "nx affected:apps",
    "affected:libs": "nx affected:libs",
    "affected:build": "nx affected:build",
    "affected:e2e": "nx affected:e2e",
    "affected:test": "nx affected:test",
    "affected:lint": "nx affected:lint",
    "affected:dep-graph": "nx affected:dep-graph",
    "affected": "nx affected",
    "format": "nx format:write",
    "format:write": "nx format:write",
    "format:check": "nx format:check",
    "update": "nx migrate latest",
    "dep-graph": "nx dep-graph",
    "help": "nx help",
    "workspace-generator": "nx workspace-generator"
  },
